{
    "name": "olcs/olcs-logging",
    "description": "Logging Module for OLCS",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "psr/log": "^1.0",
        "laminas/laminas-log": "^2.12",
        "laminas/laminas-eventmanager": "^2.6.4|^3.0",
        "laminas/laminas-mvc": "^2.7.15|^3.3",
        "laminas/laminas-session": "^2.8.7",
        "laminas/laminas-console": "^2.7",
        "laminas/laminas-stdlib": "^2.7.9|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Olcs\\Logging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OlcsTest\\Logging\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "mockery/mockery": "^1.6",
        "johnkary/phpunit-speedtrap": "^4.0",
        "bamarni/composer-bin-plugin": "^1.8",
        "dvsa/coding-standards": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    }
}
