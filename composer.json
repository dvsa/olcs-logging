{
    "name": "olcs/olcs-logging",
    "description": "Logging Module for Olcs",
    "repositories": [
        {
            "packagist": false
        },
        {
            "url": "http://devrepo.shd.ci.nonprod.dvsa.aws",
            "type": "composer"
        },
        {
            "url": "git@repo.shd.ci.nonprod.dvsa.aws:olcs/olcs-logging.git",
            "type": "vcs"
        }
    ],
    "require": {
        "php": ">=5.4",
        "psr/log": "~1.0",
        "zendframework/zend-log": "~2.4.0",
        "zendframework/zend-servicemanager": "~2.4.0",
        "zendframework/zend-eventmanager": "~2.4.0",
        "zendframework/zend-session": "~2.4.0",
        "zendframework/zend-mvc": "~2.4.0",
        "zendframework/zend-http": "~2.4.0",
        "zendframework/zend-console": "~2.4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.5.3",
        "phpunit/phpunit": "^4.4.0",
        "mockery/mockery": "~0.9.3",
        "phpunit/phpcov": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Olcs\\Logging\\": "src//"
        }
    },
    "config": {
      "preferred-install": "dist",
      "secure-http" : false
    }
}
