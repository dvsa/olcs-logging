{
    "name": "olcs/olcs-logging",
    "description": "Logging Module for Olcs",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MajorCaiger/phpcov.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "psr/log": "~1.0",
        "zendframework/zend-log": "~2.3.1",
        "zendframework/zend-servicemanager": "~2.3.1",
        "zendframework/zend-eventmanager": "~2.3.1",
        "zendframework/zend-session": "~2.3.1",
        "zendframework/zend-mvc": "~2.3.1",
        "zendframework/zend-http": "~2.3.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.5.3",
        "phpunit/phpunit": "4.*",
        "phpunit/phpcov": "dev-master",
        "mockery/mockery": "0.9.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Olcs\\Logging\\": "src//"
        }
    },
    "config": {
      "preferred-install": "dist",
        "github-oauth": {
          "github.com": "0a3b8b6b0ec37cd7900b532e8a48c0ec97628ef6"
      }
    }
}
